{
  "name": "@iamleo/blog-api",
  "version": "0.0.1",
  "description": "Awesome project developed with TypeORM.",
  "type": "commonjs",
  "devDependencies": {
    "@types/node": "^16.11.10",
    "concurrently": "^8.2.0",
    "nodemon": "^2.0.22",
    "ts-node": "10.7.0",
    "typescript": "^4.5.3"
  },
  "dependencies": {
    "@apollo/server": "^4.7.3",
    "dotenv": "^16.1.4",
    "pg": "^8.4.0",
    "reflect-metadata": "^0.1.13",
    "type-graphql": "^2.0.0-beta.2",
    "typeorm": "0.3.16"
  },
  "scripts": {
